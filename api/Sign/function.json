{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "post"
      ],
      "route": "sign"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "name": "signature",
      "type": "cosmosDB",
      "databaseName": "qmul-phd",
      "collectionName": "signatures",
      "connectionStringSetting": "CosmosDBConnection",      
      "direction": "out"
    }
  ]
}
